//function to remove dulicate elements




import UIKit
func changesarray(arr:[Int]) -> [Int] {
    let mainarray = arr
    var setArray = Set<Int>()
    for i in 0..<mainarray.count {
        setArray.insert(mainarray[i])
    }
   return Array(setArray)
}

let sortedArray = changesarray(arr: [3,2,4,23,43,12,43,12,1,5,3,2])
print(sortedArray)
