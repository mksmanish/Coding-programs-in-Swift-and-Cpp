//function to bubble sort




import UIKit
func changesarray(arr:[Int]) -> [Int] {
    var mainarray = arr
    for i in 0..<mainarray.count{
        for j in i..<mainarray.count {
            if(mainarray[i] > mainarray[j]) {
                mainarray.swapAt(i, j)
            }
        }
    }
   print(mainarray)
   return mainarray
}

let sortedArray = changesarray(arr: [3,2,4,23,43,12,43,12,1,5,3,2])
print(sortedArray)
